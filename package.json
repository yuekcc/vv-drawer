{
  "name": "vv-drawer",
  "version": "1.0.0",
  "main": "./dist/vv-drawer.umd.js",
  "module": "./dist/vv-drawer.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "demo": "vite -c demo.config.js",
    "build": "vite build -c lib.config.js"
  },
  "author": "zhanlefeng",
  "license": "MIT",
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "commonjs": true,
      "browser": true,
      "node": true,
      "worker": true,
      "mocha": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "prettier": "^2.0.0",
    "sass": "^1.57.1",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "vue": "^3.2.45"
  }
}
